--1create the database 
CREATE TABLE "CASIS_MGR"."CASIS_DOCUMENT_JUNITS" 
   (	"DOCNO" VARCHAR2(30 BYTE), 
	"SRC_DB" VARCHAR2(10 BYTE), 
	"PART" NUMBER, 
	"UPD" NUMBER(12,0), 
	"DOCUMENT" CLOB, 
	"DATEINSERTED" DATE ,
	"ID" NUMBER(19,0), 
	"VERSION" NUMBER(10,0), 
	"UPDATE_TIMESTAMP" TIMESTAMP (6))
SEGMENT CREATION DEFERRED PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING TABLESPACE "CASIS" ;


